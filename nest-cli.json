{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/basic-nestjs/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "plugins": [
      "@nestjs/swagger/plugin"
    ],
    "webpack": true,
    "tsConfigPath": "apps/basic-nestjs/tsconfig.app.json"
  },
  "monorepo": true,
  "root": "apps/basic-nestjs",
  "projects": {
    "basic-nestjs": {
      "type": "application",
      "root": "apps/basic-nestjs",
      "entryFile": "main",
      "sourceRoot": "apps/basic-nestjs/src",
      "compilerOptions": {
        "tsConfigPath": "apps/basic-nestjs/tsconfig.app.json"
      }
    },
    "authentication": {
      "type": "application",
      "root": "apps/authentication",
      "entryFile": "main",
      "sourceRoot": "apps/authentication/src",
      "compilerOptions": {
        "tsConfigPath": "apps/authentication/tsconfig.app.json"
      }
    }
  }
}